<?xml version="1.0" encoding="UTF-8" ?>
<!-- Configuration file for application. Description of each property is explained in cz.semenko.word.Config -->
<config>
	<application>
		<name>DeterPropText</name>
	</application>
	<application>
<!-- 		<databaseHome>c:\\Users\\k\\DeterPropText</databaseHome> -->
		<databaseHome>c:\\Users\\k\\Deeplearning2</databaseHome>
	</application>
	<dataProvider>
		<numCharsReadsFromInput>400</numCharsReadsFromInput><!-- Tested: 512 - 1 sek., 4 - 26 sek. --> 
	</dataProvider>
	<fastMemory>
		<tablesTableSize>400</tablesTableSize>
		<tablesCellsSize>1000</tablesCellsSize>
		<tablesAssociationsSize>1000</tablesAssociationsSize>
		<alwaysSearchToAssociationsDeepInTheMemory>false</alwaysSearchToAssociationsDeepInTheMemory>
		<searchToAssociationsAtAllElements>true</searchToAssociationsAtAllElements>
	</fastMemory>
	<knowledge>
<!-- 		<relateThoughtsUpToCellType>68</relateThoughtsUpToCellType> -->
		<knowledgeSize>100</knowledgeSize>
		<relateOnlyCellsOfTheSameTypes>false</relateOnlyCellsOfTheSameTypes>
		<decideToRelateCellsByHigherCellType>false</decideToRelateCellsByHigherCellType>
		<decideToRelateCellsByHigherAssocCost>true</decideToRelateCellsByHigherAssocCost>
		<minAssocCostToRelate>10</minAssocCostToRelate>
		<saveThoughtsToFile>true</saveThoughtsToFile>
	</knowledge>
	<cellsCreationDecider>
		<createNewCellsToAllPairsDepth>30</createNewCellsToAllPairsDepth>
	</cellsCreationDecider>
	<thoughtsSaver>
		<filePathToSaveThoughts>
			Data-out\ulozenaIdeckaBehemZpracovani.txt
		</filePathToSaveThoughts>
	</thoughtsSaver>
	<memoryCleaner>
		<lowestCostForLeaving>2</lowestCostForLeaving>
	</memoryCleaner>
	<dbViewer>
		<maxTextLengthToSave>30</maxTextLengthToSave>
		<numRowsForCleanupRotation>100</numRowsForCleanupRotation>
		<numberOfAvailableCellsIdToReturn>200</numberOfAvailableCellsIdToReturn>
		<numberOfAvailableAssociationsIdToReturn>200</numberOfAvailableAssociationsIdToReturn>
	</dbViewer>
	<thoughtUnionDecider>
		<competitionAllowed>true</competitionAllowed>
	</thoughtUnionDecider>
</config>
